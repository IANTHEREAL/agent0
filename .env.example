# Copy to `.env` and edit as needed.
# Values here are examples only.

# Pantheon MCP SSE endpoint.
# Example: http://localhost:8000/mcp/sse
MCP_BASE_URL=http://localhost:8000/mcp/sse

# Project name. The CLI reads the first non-empty of:
#   PANTHEON_PROJECT_NAME, MCP_PROJECT_NAME, PROJECT_NAME
PANTHEON_PROJECT_NAME=agent0
MCP_PROJECT_NAME=
PROJECT_NAME=

# First-run only: baseline branch ID to start from.
PANTHEON_PARENT_BRANCH_ID=
MCP_PARENT_BRANCH_ID=

# Agent name registered in Pantheon (default: codex).
PANTHEON_AGENT=codex
MCP_AGENT=

# Optional bootstrap hints (used on first run when controller state is uninitialized).
AGENTS_MD_URL=
SKILLS_URL=
MINIBOOK_ACCOUNT=
